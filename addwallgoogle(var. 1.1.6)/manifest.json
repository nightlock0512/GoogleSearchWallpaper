{
    "name": "GoogleSearchWallpaper",
    "description": "",
    "version": "1.1.6",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search*",
                "https://www.bing.com/search*"
            ],
            "css": [
                "main.css"
            ],
            "js":[
                "main.js"
            ]
        }
    ],
    "options_page": "options.html"
}   